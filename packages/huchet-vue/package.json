    "@nuxt/kit": "^3.14.1592",
    "@nuxt/schema": "^3.14.1592",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^22.9.3",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vue/tsconfig": "^0.6.0",
    "jsdom": "^25.0.1",
    "typescript": "^5.7.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.5",
    "vue": "^3.5.13"
